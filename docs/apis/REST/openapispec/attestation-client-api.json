      "get": {
        "description": "Requests for an attestation proof status.",
        "operationId": "ProofController_systemStatus",
        "parameters": [],
        "responses": {
          "200": {
            "description": "Returns an attestation proof status.",
            "content": {
              "application/json": {
                "schema": {
                  "allOf": [
                    {
                      "$ref": "#/components/schemas/ApiResponseWrapper"
                    },
